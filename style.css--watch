footer, .headerGreen {
  background-color: #19947B;
  height: 100px;
}

.headerGreen {
  margin: auto;
}
.headerGreen h1 {
  font-family: Helvetica, sans-serif;
  color: #FDC100;
  display: flex;
  justify-content: center;
  padding: 25px;
  font-size: 2.5em;
}

.btn, button {
  padding: 10px 25px;
  font-size: 2em;
  border-radius: 10px;
  display: flex;
  margin: 40px auto;
  background: #19947B;
  color: #FDC100;
}

button {
  position: relative;
}

* {
  transition: all 200ms ease-in-out;
}

.prochainement {
  font-size: 40px;
  width: 800px;
  overflow: hidden;
  animation: 2s typing steps(50, end) infinite;
}

@keyframes typing {
  0% {
    opacity: 0;
  }
  100% {
    opacity: 1;
  }
}
@keyframes typing {
  from {
    width: 0;
  }
}
footer {
  display: flex;
  justify-content: space-around;
  align-items: center;
  color: #FDC100;
  font-size: 1.5em;
}

