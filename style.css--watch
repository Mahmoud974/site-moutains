/*Taille d'ecran responsive*/
html,
body,
ul,
ol,
li,
h1 {
  margin: 0;
  padding: 0;
  font-family: Helvetica, sans-serif;
}

article .container {
  display: flex;
  flex-direction: row;
  background-color: white;
  justify-content: center;
}

.menu-1b {
  width: 300px;
  margin: 30px;
  box-shadow: 8px 8px 20px #999999;
  opacity: 1;
  animation-name: shake;
}
.menu-1b:hover {
  opacity: 0.5;
  box-shadow: 0;
  animation-duration: 1s;
  animation-iteration-count: infinite;
}
@keyframes shake {
  10%, 30%, 50%, 70%, 90% {
    transform: translate3d(-10px, 0, 0);
  }
  20%, 40%, 60%, 80% {
    transform: translate3d(10px, 0, 0);
  }
}

@media only screen and (max-width: screen-iphoneQuatre) {
  .menu-1b {
    width: 100px;
    margin: 10px 5px;
    cursor: pointer;
  }
}
@media only screen and (max-width: 576px) {
  .menu-1b {
    width: 290px;
    margin: 20px 10px;
  }
}
@media only screen and (max-width: 576px) {
  .menu-1b {
    width: 200px;
    margin: 30px 10px;
  }
}
@media only screen and (min-width: 700px) {
  .menu-1b {
    width: 100px;
    margin: 30px 10px;
  }
}
@media only screen and (max-width: 700px) {
  .menu-1b {
    width: 200px;
    margin: 30px 10px;
  }
}
@media only screen and (max-width: 700px) {
  .menu-1b {
    width: 180px;
    margin: 30px 15px;
  }
}
@media only screen and (min-width: 700px) {
  .menu-1b {
    width: 280px;
    margin: 30px 15px;
  }
}
@media only screen and (min-width: 800px) {
  .menu-1b {
    width: 300px;
    margin: 30px;
  }
}
